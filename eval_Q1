def compare_string(s1,s2):
    commaon_chars=''
    uncommon_chars=''
    len1=len(s1)
    len2=len(s2)
    i,j=0,0
    while i<len1 and j<len2:
        if s1[i]==s2[j]:
            common_chars+=s1[i]
            i+=1
            j+=1
            else:
                uncommon_chars+=s1[i]
                i+=1
    while i<len1:
        uncommon_chars+=s1[i]
        i+=1
    while j<len2:
        uncommon_chars+=s2[j]
        j+=1
    print("common_characters:",common_chars)
    print("uncomon characters:",uncommon_chars)
s1="Refer"
s2="Rafael"
compare_strings(s1,s2)
